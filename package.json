{
  "name": "demo-api",
  "version": "1.0.0",
  "description": "simple demo api",
  "scripts": {
    "format": "prettier --single-quote --write .",
    "start": "env-cmd -e local sls offline start",
    "start:debug": "env-cmd -e local npm run debug",
    "debug": "node --inspect node_modules/serverless/bin/serverless offline -s dev",
    "package": "sls package",
    "tsc:compile": "tsc",
    "test:clean": "rimraf ./coverage",
    "test": "npm run test:clean && jest -c jest.config.js",
    "test:clearcache": "jest --clearCache",
    "lint": "eslint --ext .ts . --ignore-path .gitignore",
    "lint:fix": "eslint --ext .ts . --ignore-path .gitignore --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@middy/core": "^2.5.7",
    "@middy/http-cors": "^2.5.7",
    "@middy/http-error-handler": "^2.5.7",
    "@middy/http-json-body-parser": "^2.5.7",
    "@middy/validator": "^2.5.7",
    "nodemailer": "^6.7.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "^27.4.1",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.1.0",
    "prettier": "^2.6.2",
    "serverless": "^3.12.0",
    "serverless-offline": "^8.5.0",
    "serverless-webpack": "^5.6.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-node-externals": "^3.0.0"
  }
}
